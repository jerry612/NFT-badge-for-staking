{
  "name": "0xHack-chainlink-on-polygon",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "truffle test ./test/test-local/* --network local",
    "test:Boson-General-Scenario": "truffle test ./test/test-local/Boson-general-scenario.test.js --network local",
    "test:boson-scenario": "truffle test ./test/test-local/boson-scenario.test.js --network local",
    "script:boson-scenario": "truffle exec ./scripts/polygon-mumbai/boson-scenario.script.js --network polygon_mumbai",
    "compile:local": "truffle compile --reset --network local",
    "migrate:local": "truffle migrate --reset --network local",
    "compile:polygon_mumbai": "truffle compile --reset --network polygon_mumbai",
    "migrate:polygon_mumbai": "truffle migrate --reset --network polygon_mumbai",
    "hardhat:node": "npx hardhat node",
    "hardhat-test:boson-x-chain": "truffle test ./test/test-local/boson-x-chain/*  --network local",
    "hardhat-test:boson": "truffle test ./test/test-local/boson/*  --network local"
  },
  "author": "masaun",
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/contracts": "3.4.1",
    "@openzeppelin/test-helpers": "^0.5.10",
    "@truffle/hdwallet-provider": "^1.2.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eth-block-tracker": "^4.4.3",
    "ethers": "5.0.32",
    "hardhat": "^2.3.0",
    "truffle-assertions": "^0.9.2"
  }
}
